{
  "name": "NameCheck for Gmail",
  "description": "Always spell contact names correctly in your emails!",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["lib/jquery-3.1.0.min.js","lib/lodash.core.min.js","classes.js","inboxsdk.js","content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2
}
